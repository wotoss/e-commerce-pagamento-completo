{
  "iisSettings": {
    "windowsAuthentication": false,
    "anonymousAuthentication": true,
    "iisExpress": {
      "applicationUrl": "http://localhost:50101",
      "sslPort": 44308
    }
  },

  //CONTRUINDO ESTES AMBIENTES EU CONSIGO SIMULAR NA MINHA MAQUINA O AMBIENTE DE DESENVOLVIMENTO, PRODUÇÃO E PRE-PRODUÇAÕ


  // 1º configuração é para o (IIS Express)
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "profiles": {
    "IIS - Development": { //Eu criei o (IIS-Development)
      "commandName": "IISExpress",
      "launchBrowser": true,
      "launchUrl": "swagger",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },

    // 2º Stage é um ambiente para (pre-produção)
    "IIS - Staging": {
      "commandName": "IISExpress",
      "launchBrowser": true,
      "launchUrl": "swagger",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Staging"
      }
    },

    // 2º Production é um ambiente de (Produção)
    "IIS - Production": {
      "commandName": "IISExpress",
      "launchBrowser": true,
      "launchUrl": "swagger",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Production"
      }
    },

    // 2º Self host
    "Selfhosting - Development": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "swagger",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "applicationUrl": "https://localhost:5001;http://localhost:5000"
    },
    // 3º Configuração do Docker
    "Docker": {
      "commandName": "Docker",
      "launchBrowser": true,
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}/swagger",
      "publishAllPorts": true,
      "useSSL": true
    }
  }
}